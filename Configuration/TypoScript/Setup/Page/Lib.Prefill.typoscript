lib {
  prefillUrl = TEXT
  prefillUrl {
    dataWrap = {getIndpEnv:TYPO3_REQUEST_HOST}|
    typolink {
      parameter.current = 1
      returnLast = url
    }
  }
  currentURL = TEXT
  currentURL {
    data = getIndpEnv:TYPO3_REQUEST_URL
  }
}
