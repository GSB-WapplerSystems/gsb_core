{
  "root": true,
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {},
  "rules": {
    "no-console": [
      "warn"
    ],
    "object-curly-spacing": [
      2,
      "never"
    ]
  }
}