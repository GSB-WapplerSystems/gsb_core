{
  "name": "bootstrap5-webpack",
  "version": "1.0.0",
  "description": "Bootstrap 5, Webpack, PostCSS, Stylelint and Eslint.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.config.production.js --mode production",
    "dev": "webpack --config webpack.config.development.js --mode development",
    "watch": "Webpack --config webpack.config.development.js --mode development --watch",
    "lint:eslint": "eslint Assets/**/*.js",
    "lint:eslint.fix": "eslint --fix Assets/**/*.js",
    "lint:eslint.report.junit": "eslint -f junit -o reports/eslintreport.junit.xml Assets/**/*.js",
    "lint:stylelint.report.json": "stylelint --config stylelint.config.js -f json -o reports/stylelintreport.json Assets/Scss/**/*.scss",
    "lint:stylelint.report.junit": "stylelint --config stylelint.config.js --custom-formatter 'node_modules/stylelint-junit-formatter' -o reports/stylelintreport.junit.xml Assets/Scss/**/*.scss",
    "lint:stylelint.report.verbose": "stylelint --config stylelint.config.js -f verbose -o reports/stylelintreport.verbose.txt Assets/Scss/**/*.scss"
  },
  "author": "Matthias Peltzer",
  "license": "ISC",
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "mediaelement": "^5.1.0",
    "swiper": "^8.4.5"
  },
  "devDependencies": {
    "@andreyvolokitin/postcss-inline-svg": "^5.0.1",
    "autoprefixer": "^10.4.13",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.28.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^2.7.0",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.3",
    "postcss-pxtorem": "^6.0.0",
    "postcss-svgo": "^5.1.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.15.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-junit-formatter": "^0.2.2",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "xml2js": "^0.4.23"
  }
}
